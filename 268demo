#include<stdio.h>
int main() {
//input
	int mid,final,q1,q2,lab,sit;
	scanf("%d %d %d %d %d %d",&mid,&final,&q1,&q2,&lab,&sit);
//cal
	float Mid,Final,Q1,Q2,Lab,Sit,Total;
	Mid=(mid*30.0)/100.0;
	Final=(final*35.0)/120.0;
	Q1=(q1*12.0)/60.0;
	Q2=(q2*13.0)/60.0;
	Lab=(lab*5.0)/16.0;
	Sit=(sit*5.0)/16.0;
	Total=(Mid+Final+Q1+Q2+Lab+Sit);
//output
	printf("%10s=%8d=>%.2f\n","Mid",mid,Mid);
	printf("%10s=%8d=>%.2f\n","Final",final,Final);
	printf("%10s=%8d=>%.2f\n","Q1",q1,Q1);
	printf("%10s=%8d=>%.2f\n","Q2",q2,Q2,Lab,Sit,Total);
	printf("%10s=%8d=>%.2f\n","Lab",lab,Lab);
	printf("%10s=%8d=>%.2f\n","Sit",sit,Sit);
	printf("*****%.2f*****",Total);
return 0;
}
